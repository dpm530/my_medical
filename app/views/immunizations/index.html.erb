<p id="notice"><%= notice %></p>

<h1>Immunizations</h1>

<table>
  <thead>
    <tr>
      <th>Vaccine</th>
      <th>Received on</th>
      <th>Expired on</th>
      <th>Administered by</th>
      <th>Dosage</th>
      <th>Manufacturer</th>
      <th>Note</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @immunizations.each do |immunization| %>
      <tr>
        <td><%= immunization.vaccine %></td>
        <td><%= immunization.received_on %></td>
        <td><%= immunization.expired_on %></td>
        <td><%= immunization.administered_by %></td>
        <td><%= immunization.dosage %></td>
        <td><%= immunization.manufacturer %></td>
        <td><%= immunization.note %></td>
        <td><%= immunization.patient %></td>
        <td><%= link_to 'Show', immunization %></td>
        <td><%= link_to 'Edit', edit_immunization_path(immunization) %></td>
        <td><%= link_to 'Destroy', immunization, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Immunization', new_immunization_path %>
