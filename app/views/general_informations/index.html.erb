<p id="notice"><%= notice %></p>

<h1>General Informations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @general_informations.each do |general_information| %>
      <tr>
        <td><%= link_to 'Show', general_information %></td>
        <td><%= link_to 'Edit', edit_general_information_path(general_information) %></td>
        <td><%= link_to 'Destroy', general_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New General Information', new_general_information_path %>
