<p id="notice"><%= notice %></p>

<h1>Family Histories</h1>

<table>
  <thead>
    <tr>
      <th>First name</th>
      <th>Last name</th>
      <th>Relationship</th>
      <th>Dob</th>
      <th>Dod</th>
      <th>Medical history</th>
      <th>Patient</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @family_histories.each do |family_history| %>
      <tr>
        <td><%= family_history.first_name %></td>
        <td><%= family_history.last_name %></td>
        <td><%= family_history.relationship %></td>
        <td><%= family_history.dob %></td>
        <td><%= family_history.dod %></td>
        <td><%= family_history.medical_history %></td>
        <td><%= family_history.patient %></td>
        <td><%= link_to 'Show', family_history %></td>
        <td><%= link_to 'Edit', edit_family_history_path(family_history) %></td>
        <td><%= link_to 'Destroy', family_history, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Family History', new_family_history_path %>
